<h1>New Article</h1>

<%= form_tag scope: :article, url: articles_path, local: true do |form| %>
  <p>
    <%= label_tag(:title) %><br>
    <%= text_field_tag(:title) %>
  </p>

 <p>
    <%= label_tag(:text) %><br>
    <%= text_area_tag(:text) %><br>
	<%= submit_tag(:submit) %>
  </p>   
<% end %>

